{
  "name": "drouple",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "dotenv -e .env.test -- vitest run",
    "test:unit:watch": "dotenv -e .env.test -- vitest watch",
    "test:unit:coverage": "dotenv -e .env.test -- COVERAGE=true vitest run",
    "coverage:summary": "tsx scripts/print-coverage-summary.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "CI=true playwright test",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:load": "artillery run tests/load/artillery.yml",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "seed": "prisma db push --force-reset && tsx prisma/seed.ts",
    "test-email": "dotenv -e .env.local -- tsx scripts/test-email.ts",
    "seed:verify": "tsx scripts/verify-seeds.ts",
    "env:sanity": "tsx scripts/env-sanity.ts",
    "analyze": "ANALYZE=true next build --config next.config.bundle.js",
    "db:test:up": "docker compose -f docker-compose.test.yml up -d",
    "db:test:down": "docker compose -f docker-compose.test.yml down -v",
    "test:prepare": "dotenv -e .env.test -- npx prisma migrate deploy && dotenv -e .env.test -- npm run seed",
    "test:e2e:local": "dotenv -e .env.test -- npm run test:e2e",
    "ship:verify": "npm run typecheck && npm run lint && npm run test:unit && npm run test:e2e && npm run build && echo 'ðŸš€ All checks passed!'",
    "migrate:production": "tsx scripts/migrate-production.ts",
    "migrate:backup": "tsx scripts/backup-strategy.ts create",
    "migrate:validate": "tsx scripts/migrate-production.ts --dry-run",
    "backup:create": "tsx scripts/backup-strategy.ts create",
    "backup:incremental": "tsx scripts/backup-strategy.ts incremental",
    "backup:list": "tsx scripts/backup-strategy.ts list",
    "backup:restore": "tsx scripts/backup-strategy.ts restore",
    "backup:cleanup": "tsx scripts/backup-strategy.ts cleanup",
    "backup:test-restore": "tsx scripts/backup-strategy.ts restore --dry-run",
    "db:status": "npx prisma migrate status",
    "db:health": "node -e \"require('./lib/monitoring').performHealthCheck().then(console.log)\"",
    "monitoring:test": "node -e \"require('./lib/alerts').createAlert('info', 'Test Alert', 'This is a test alert', 'system')\"",
    "security:audit": "npm audit --audit-level=high",
    "performance:analyze": "npm run analyze && echo 'Check .next/analyze for bundle analysis'",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:status": "./scripts/check-deployment.sh",
    "vercel:status": "vercel ls",
    "vercel:logs": "vercel logs",
    "test:production": "tsx scripts/production-testing/run-production-tests.ts",
    "test:production:headless": "tsx scripts/production-testing/run-production-tests.ts --headless",
    "test:production:admins": "tsx scripts/production-testing/run-production-tests.ts --accounts \"admin.manila@test.com,admin.cebu@test.com\"",
    "test:production:auth": "tsx scripts/production-testing/run-production-tests.ts --scenarios \"Authentication Flow\"",
    "test:production:setup": "cd scripts/production-testing && npm install",
    "test:pwa:lighthouse": "node tests/run-lighthouse.js",
    "test:pwa:playwright": "playwright test tests/pwa.spec.ts --config tests/playwright.config.ts",
    "test:pwa": "node tests/run-pwa-audit.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@drouple/contracts": "file:./packages/contracts",
    "@hookform/resolvers": "^3.10.0",
