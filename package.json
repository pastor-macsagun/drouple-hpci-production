    "firebase-admin": "^13.5.0",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.468.0",
    "nativewind": "^4.1.23",
    "next": "^15.5.4",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.9.17",
    "openapi-fetch": "^0.14.0",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native-reanimated": "^4.1.0",
    "redis": "^5.8.2",
    "resend": "^4.0.1",
    "socket.io": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
